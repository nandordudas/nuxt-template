{
  "name": "@workspace/website",
  "type": "module",
  "version": "0.3.0",
  "private": true,
  "description": "Opinionated Nuxt.js template package for workspace",
  "scripts": {
    "build": "nuxt build",
    "cleanup": "nuxt cleanup",
    "dev": "nuxt dev --dotenv .env.local",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare; [ -f .env.local ] || cp .env.example .env.local",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck",
    "upgrade": "nuxt upgrade --dedupe"
  },
  "dependencies": {
    "@iconify-json/lucide": "catalog:prod",
    "@nuxt/ui-pro": "catalog:prod",
    "nuxt": "catalog:prod",
    "vue": "catalog:prod",
    "vue-router": "catalog:prod"
  },
  "devDependencies": {
    "@pinia/nuxt": "catalog:dev",
    "vue-tsc": "catalog:lint"
  }
}
